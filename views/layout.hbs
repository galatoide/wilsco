<!-- CSSMap SCRIPT -->
<script type="text/javascript" src="https://cssmapsplugin.com/5/jquery.cssmap.min.js"></script>

<script type="text/javascript">
  $(document).ready(function () {

    // CSSMap;
    $("#map-continents").CSSMap({
      "size": 1450,
      "mapStyle": "blue",
      "cities": true,
      "tooltips": "floating-top-center",
      "responsive": "auto",
      "fitHeight": false,
      "tapOnce": true,
      "navigation": {
        "enable": false,
        "label": "",
      },
      "authorInfo": false
    });
    // END OF THE CSSMap;

  });
  // to display the date in the dashboard

  document.getElementById("date").innerHTML = new Date().toUTCString("m");

</script>

